import java.util.Scanner;
public class Задан {
	public static void main(String argsp[]){
	      Scanner in = new Scanner(System.in);
	      System.out.print("Доволен ли гусь своим существованием? Да - 1; Нет - 2 ");
	    int a = in.nextInt();
	    if (a==1){
	    	System.out.println("Тогда оставьте его в покое");}	    
	    else {
	     System.out.println("Тогда отправьте его погулять");
	}}}
